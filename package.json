{
	"name": "tpyo",
	"version": "2.0.0",
	"description": "A simple ES6 proxy wrapper that enables you to make typos in property names.",
	"homepage": "https://mths.be/tpyo",
	"main": "tpyo.js",
	"keywords": [
		"es6",
		"proxy",
		"proxies",
		"levenshtein",
		"levenshtein-distance",
		"string",
		"property",
		"property-names",
		"properties",
		"typo",
		"typos"
	],
	"license": "MIT",
	"author": {
		"name": "Mathias Bynens",
		"url": "https://mathiasbynens.be/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mathiasbynens/tpyo.git"
	},
	"bugs": "https://github.com/mathiasbynens/tpyo/issues",
	"files": [
		"LICENSE-MIT.txt",
		"tpyo.js"
	],
	"scripts": {
		"test": "node --harmony-proxies tests/tests.js",
		"cover": "node --harmony-proxies ./node_modules/istanbul/lib/cli.js cover --report html --verbose --dir coverage ./tests/tests.js"
	},
	"dependencies": {
		"levenshtein": "^1.0.2"
	}
}
